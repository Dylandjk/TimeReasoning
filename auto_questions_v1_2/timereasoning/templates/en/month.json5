// 以月为单位的命题陈述模板
// 模板中存在占位符，格式为"[占位符名]"，如"[element]"、"[time]"等
// 占位符与相应命题的属性对应

{
    // 瞬时事件的模板
    "temporal": [
        "He [element], and that was in month [time]",
        "In month [time], He [element]",
    ],
    // 时长的模板
    "duration": [
        "He [element] for [duration] month(s)",
        "He [element], and spent [duration] month(s) on it",
        "It takes [duration] month(s) that he [element]",
        "He [element], until [duration] month(s) later",
        "For [duration] month(s), he [element]",
    ],
    // 持续事件的模板
    "durative": [
        // 描述起点和终点（同年）
        "From month [time] to month [endtime], he [element]",
        "In month [time] he [element], until month [endtime]",
        "He [element] in month [time], and ended in month [endtime]",
        "He [element] from month [time] to month [endtime]",
        "month [time] to month [endtime], he [element]",
        // 描述起点和持续时间
        "From month [time] on, he [element] for [duration] month(s)",
        "Starting from month [time], he [element] for [duration] month(s)",
        "He [element] since month [time], for [duration] month(s)",
        // 描述终点和持续时间
        "He [element] for [duration] month(s) before month [endtime]",
        "By month [endtime], he [element] for [duration] month(s)",
        "Before month [endtime], it's been [duration] month(s) that he [element]",
    ],
    // 时频的模板
    "freq": [
        "From month [time] to month [endtime], he [element] once every [frequency] month(s)",
        "Over the [duration] month(s) starting from month [time], he [element] once every [frequency] month(s)",
        "In the [duration] month(s) before month [endtime], he [element] once every [frequency] month(s)",
    ],
    // 事件先后关系的模板
    "before": [
        "He [element1] earlier than [element2]",
        "He [element1] before he [element2]",
    ],
    "before_time": [
        "He [element1] [diff] month(s) earlier than he [element2]",
        "He [element1] [diff] month(s) before he [element2]",
        "[diff] month(s) before he [element2], he [element1]",
    ],
    "after": [
        "He [element1] later than [element2]",
        "He [element1] after he [element2]",
    ],
    "after_time": [
        "He [element1] [diff] month(s) later than he [element2]",
        "He [element1] [diff] month(s) after he [element2]",
        "[diff] month(s) after he [element2], he [element1]",
    ],
    "simultaneous": [
        "In the month that he [element2], he also [element1]",
        "In the same month, he not only [element2], but also [element1]",
        "In the same month as he [element2], he also [element1]",
        "In a certain month, he both [element2] and [element1]",
    ],
    // 事件间隔的模板
    "gap_time": [
        "There are [diff] month(s) between he [element2] and [element1]",
        "The time gap between he [element2] and [element1] is [diff] month(s)",
        "He has a [diff]-month difference between he [element2] and [element1]"
    ],
    // 时长关系的模板
    "long": [
        "The total duration of which he [element1] is longer than that of he [element2]",
        "It takes a longer time that he [element1] than he [element2]",
        "Compared to the time he [element2], more time is taken that he [element1]",
    ],
    "long_time": [
        "[diff] more month(s) are taken that he [element1] than he [element2]",
        "The total duration of which he [element1] is [diff] month(s) longer than that of he [element2]",
        "He takes [diff] more month(s) than he [element2] that he [element1]",
    ],
    "short": [
        "The total duration of which he [element1] is shorter than that of he [element2]",
        "It takes a shorter time that he [element1] than he [element2]",
        "Compared to the time he [element2], less time is taken that he [element1]",
    ],
    "short_time": [
        "[diff] less month(s) are taken that he [element1] than he [element2]",
        "The total duration of which he [element1] is [diff] month(s) shorter than that of he [element2]",
        "He takes [diff] less month(s) than he [element2] that he [element1]",
    ],
    "same_len": [
        "He spends the same number of month(s) that he [element1] as he [element2]",
        "The time he [element1] is the same length as that he [element2]",
        "The time he [element1] is the same as the time he [element2]",
        "The time he [element1] is equal to the time he [element2]",
        "The time he [element1] matches the time he [element2]",
        "He spends the same amount of time that he [element1] and [element2]",
    ],
    // 增加：during的模板
    "during": [
        "During the time that he [element2], he also [element1]",
        "He [element1], and that was during the time that he [element2]",
        "He [element2], and during this time he also [element1]",
        "He [element1] during the time he [element2]",
    ]
}